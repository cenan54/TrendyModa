@{
    ViewData["Title"] = "Takipçiler";
}

@model List<User>

<div class="row my-5"></div>

<partial name="ReturnProfilePage" />
<div class="row">
    <div class="col-sm-0 col-lg-2 col-xxl-3"></div>
    <div class="col-sm-12 col-lg-8 col-xxl-6">
        <h2>Takipçiler</h2>
        <ul>
            @foreach (var user in Model)
            {
                <li>@user.Username</li>
            }
        </ul>
    </div>
    <div class="col-sm-0 col-lg-2 col-xxl-3"></div>
</div>
